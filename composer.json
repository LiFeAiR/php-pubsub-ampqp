{
  "name": "delivery-club/external/php-pubsub-ampqp",
  "type": "application",
  "description": "A AMQP adapter for the php-pubsub package",
  "keywords": [
    "framework"
  ],
  "config": {
    "secure-http": false,
    "disable-tls": true
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Pavel Mozhchil",
      "email": "lifeair@gmail.com"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "http://packagist.org"
    }
  ],
  "autoload": {
    "psr-4": {
      "LiFeAiR\\PubSub\\AMQP\\": "src/",
      "Tests\\": "tests/"
    }
  },
  "require": {
    "superbalist/php-pubsub": "^1.0@dev",
    "php-amqplib/php-amqplib": "^2.9"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5",
    "mockery/mockery": "^1.0@dev"
  }
}
